{
  "name": "android-management-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.ts"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^6.28.0",
    "@mui/material": "^6.2.3",
    "@mui/icons-material": "^6.2.3",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "zustand": "^4.5.5",
    "axios": "^1.7.7",
    "@tanstack/react-query": "^5.57.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
